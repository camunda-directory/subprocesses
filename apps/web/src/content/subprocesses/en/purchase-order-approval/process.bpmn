        <di:waypoint x="720" y="193" />
        <di:waypoint x="720" y="288" />
        <di:waypoint x="800" y="288" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_9_di" bpmnElement="Flow_9">
